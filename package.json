{
  "name": "ts-dev-logger",
  "version": "0.1.4",
  "description": "A development tool that allows you to log data to the console and to the file.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "log",
    "logger",
    "file",
    "console"
  ],
  "scripts": {
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "pub": "npm version patch && npm run build && npm publish --access public",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tejks/ts-dev-logger.git"
  },
  "author": "tejks",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/tejks/ts-dev-logger/issues"
  },
  "homepage": "https://github.com/tejks/ts-dev-logger#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.6",
    "eslint": "^8.4.0",
    "mocha": "^10.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "moment": "^2.29.4"
  },
  "exclude": [
    "./tests/",
    "./node_modules/"
  ]
}
